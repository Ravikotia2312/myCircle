

<canvas id="myChart" style="width:1000%;max-width:110%"></canvas>

<script>
const xValues = [{{data}}];
const yValues = [{{dates}}];
const savedArray = [{{saved}}]
new Chart("myChart", {
    type: "line",
    data: {
      labels: yValues,  
      
      datasets: [{
        label: "Total Uploaded Posts per 1/2 hour",
        backgroundColor:"rgba(0, 0, 102,1.0)",
        borderColor: "rgba(0,0,255,0.1)",
        data: xValues
      },
      {
        label: "Total Saved Posts per 1/2 hour",
        backgroundColor:"rgba(245, 0, 0, 1.0)",
        borderColor: "rgba(0,0,255,0.1)",
        data:savedArray
      }]
    },
    
  });</script>
   
